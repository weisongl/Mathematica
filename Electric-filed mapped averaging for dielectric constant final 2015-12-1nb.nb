     RowBox[{
      RowBox[{"Exp", "[", 
       RowBox[{"\[Minus]", "\[Beta]Ex\[Mu]"}], "]"}], 
      RowBox[{"\[DifferentialD]", "x"}]}]}], " ", "=", " ", 
    RowBox[{"2", " ", 
